{
	"name": "Rate My TCU Professors",
	"version": "2.2.1",
	"description": "Rate My Professors Plugin for TCU Class Search. Allowing you to directly view professor ratings on the TCU Class Search page.",
	"author": "khoadoanduy05@gmail.com",
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [
		{
			"matches": [
				"https://classes.tcu.edu/"
			],
			"css": [
				"content/App.css"
			]
		},
		{
			"matches": [
				"https://classes.tcu.edu/"
			],
			"js": [
				"content/jquery-3.5.1.min.js",
				"content/string-similarity.min.js",
				"content/content.js"
			]
		}
	],
	"host_permissions": [
		"https://classes.tcu.edu/",
		"https://*.tcu.edu/*",
		"https://www.ratemyprofessors.com/*"
	],
	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"manifest_version": 3
}